{
    "name": "oxygen/mod-import-export",
    "description": "Import/Export Content",
    "license": "MIT",
    "keywords": [
        "cms",
        "framework",
        "OxygenExtension"
    ],
    "authors": [
        {
            "name": "Chris Chamberlain",
            "email": "chris@chamberlain.id.au"
        }
    ],
    "require": {
        "php": ">=7.2.0",
        "oxygen/core": "~0.7.0",
        "oxygen/crud": "~0.6.0",
        "illuminate/support": "~6.0",
        "ext-zip": "^1.15"
    },
    "autoload": {
        "psr-4": {
            "OxygenModule\\ImportExport\\": "src/"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "title": "Oxygen Module - Import/Export",
        "providers": [
            {
                "name": "Import/Export",
                "class": "OxygenModule\\ImportExport\\ImportExportServiceProvider",
                "description": "Import/Export Content"
            }
        ]
    },
    "require-dev": {
        "phpstan/phpstan": "^0.12.18"
    }
}
